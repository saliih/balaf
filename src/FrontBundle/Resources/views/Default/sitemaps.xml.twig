<?xml version='1.0' encoding='UTF-8'?>
<urlset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:image="http://www.google.com/schemas/sitemap-image/1.1" xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd http://www.google.com/schemas/sitemap-image/1.1 http://www.google.com/schemas/sitemap-image/1.1/sitemap-image.xsd" xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <url>
        <loc>https://www.tounsia.net{{ path('front_homepage') }}</loc>
        <lastmod>2016-06-24</lastmod>
    </url>
    <url>
        <loc>https://www.tounsia.net/fr/ramadan2017</loc>
        <lastmod>2017-02-17</lastmod>
    </url>
    <url>
        <loc>https://www.tounsia.net/fr/ramadan2018</loc>
        <lastmod>2018-05-14</lastmod>
    </url>
    <url>
        <loc>https://www.tounsia.net/fr/dbaretelyoum</loc>
        <lastmod>{{ "now" | date('Y-m-d') }}</lastmod>
    </url>
    {% for item in tags %}
        <url>
            <loc>https://www.tounsia.net{{ path('front_tags',{'locale':'fr' ,'name':item.slug}) }}</loc>
            <lastmod>2018-10-14</lastmod>
        </url>
        {% if nbtags[item.id] > 1 %}
            {% for i in 2..nbtags[item.id] %}
                <url>
                    <loc>https://www.tounsia.net{{ path('front_tags',{'locale':'fr' ,'name':item.slug}) }}?page={{ i }}</loc>
                    <lastmod>2018-10-14</lastmod>
                </url>
            {% endfor %}
        {% endif %}
    {% endfor %}
    {% for item in categories %}
    <url>
        <loc>https://www.tounsia.net{{ path('front_category',{'locale':'fr' ,'slug':item.slug}) }}</loc>
        <lastmod>2016-06-04</lastmod>
    </url>
	{% if nbPosts[item.id]>1 %}
	{% for i in 2..nbPosts[item.id] %}
	<url>
        <loc>https://www.tounsia.net{{ path('front_category',{'locale':'fr' ,'slug':item.slug}) }}?page={{ i }}</loc>
        <lastmod>2016-06-04</lastmod>
    </url>
	{% endfor %}
	{% endif %}
    {% endfor %}
    {% for item in articles %}
        {% set year =  item.publieddate|date('Y') %}
        {% set month =  item.publieddate|date('m') %}
        {% set path = path('front_article',{'locale':'fr' ,'slug':item.alias, 'year':year,'month':month,'categoryname':item.category.slug}) %}
        <url>
        <loc>https://www.tounsia.net{{ path }}</loc>
        <lastmod>{{ item.updated|date('Y-m-d') }}</lastmod>
        <image:image>
            <image:loc>https://www.tounsia.net{{ item.pic }}</image:loc>
            <image:title><![CDATA[{{ item.title }}]]></image:title>
        </image:image>
    </url>
    {% endfor %}

    <url>
        <loc>https://www.tounsia.net/echange</loc>
        <lastmod>2017-07-04</lastmod>
    </url>
</urlset>